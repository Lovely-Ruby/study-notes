{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  // "extends": ["airbnb-base", "plugin:prettier//recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "no-useless-escape": "warn"
    // "no-console": "off"
  }
}
